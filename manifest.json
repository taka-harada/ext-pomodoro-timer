{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Pomodoro Timer",
  "description": "Pomodoro Timer",
  "icons": {
    "16": "images/icons/icon_16.png",
    "32": "images/icons/icon_32.png",
    "48": "images/icons/icon_48.png",
    "128": "images/icons/icon_128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"},
  "permissions": [
    "alarms",
    "notifications",
    "storage"
  ]
}
